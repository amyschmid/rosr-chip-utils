Dummy<-newMadbSet(Slides.norm.ban)
HIST_A=paste(config$files$expDir,"/", "histAfterBAN.png",sep="")
png(HIST_A, width=7, height=7, units="in", res=300)
drawHistogram(Dummy)
dev.off()
