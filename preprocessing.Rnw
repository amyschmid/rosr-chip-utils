Dummy<-newMadbSet(Slides.norm)
HIST_B=paste(config$files$expDir,"/", "histBeforeBAN.png",sep="")
png(HIST_B, width=7, height=7, units="in", res=300)
drawHistogram(Dummy)
dev.off()
